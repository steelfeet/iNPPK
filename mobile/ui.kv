<PhotoFoodCamera>:
    name: "photo_food_camera"

    MDBoxLayout:
        # Add padding and spacing
        orientation: 'vertical'
        size_hint_y: None
        size: root.size

        MDToolbar:
            title: "Фото блюда"
            left_action_items: [["menu", lambda x: app.callback()]]
            #right_action_items: [["dots-vertical", lambda x: app.callback()], ["clock", lambda x: app.callback_2()]]
            height: dp(30)
            md_bg_color: app.theme_cls.primary_color
            font_size: 10

            MDLabel:
                id: toolbar_clock
                halign: "right"
                color: (3.0, 3.0, 3.0, 1.0)

        Image:
            source: "header.jpg"
            size_hint_y: None
            width: self.width
            allow_stretch: True


        #https://kivy.org/doc/stable/examples/gen__camera__main__py.html
        CameraClick:
            orientation: 'vertical'
            padding: 5
            spacing: 5

            Camera:
                id: camera
                resolution: (640, 480)
                play: False
            
            MDFillRoundFlatButton:
                text: 'Включить камеру'
                
                on_press: camera.play = not camera.play
                font_size: 20
                
                eight: dp(40)
                size_hint_x: 1.0
                md_bg_color: rgba("#97CF70")
                

            MDFillRoundFlatButton:
                text: 'Сфотографировать блюдо'
                
                on_press: root.capture()
                font_size: 20
                
                size_hint_x: 1.0
                size_hint_y: None
                height: dp(40)
                pos_hint: {"center_x": .5}
                md_bg_color: rgba("#E0C858")



        MDFillRoundFlatButton:
            on_press: app.screen_manager.current = 'menu'
            text: "< Назад в главное меню"
            font_size: 20
            md_bg_color: rgba("#6D7CD6")
            size_hint_x: 1.0
            size_hint_y: None
            height: dp(40)
            pos_hint: {"center_x": .5}


        Widget:    



