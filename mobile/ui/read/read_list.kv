<ReadList>:
    name: "read_list"
    _app: app

    MDBoxLayout:
        # Add padding and spacing
        orientation: 'vertical'
        size_hint_y: None
        padding: 5
        spacing: 5
        size: root.size

        MDToolbar:
            title: "Читаю - iNPPK"
            left_action_items: [["menu", lambda x: app.callback()]]
            #right_action_items: [["dots-vertical", lambda x: app.callback()], ["clock", lambda x: app.callback_2()]]
            height: dp(30)
            md_bg_color: rgba("#5C96E0")
            font_size: 10

            MDLabel:
                id: toolbar_clock
                halign: "right"
                color: (3.0, 3.0, 3.0, 1.0)

        Image:
            source: "header.jpg"
            size_hint_y: None
            width: self.width
            allow_stretch: True

        #Список результатов
        MDFillRoundFlatButton:
            text: "Лучшие 5 ссылок"
            font_size: 20
            md_bg_color: rgba("#6D7CD6")
            size_hint_x: 1.0
            size_hint_y: None
            height: dp(40)
            pos_hint: {"center_x": .5} 
            on_press: root.reload()

        MDFillRoundFlatButton:
